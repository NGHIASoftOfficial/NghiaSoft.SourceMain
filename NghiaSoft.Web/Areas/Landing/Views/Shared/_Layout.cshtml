<body>


<!--body overlay -->
<div class="body-overlay"></div>

<!--scrolltop button -->
<button class="scrolltop-btn">
    <i class="fa-solid fa-angle-up"></i>
</button>

<!--preloader start-->
@* <vc:landing-preloader></vc:landing-preloader> *@

<!--preloader end-->
<!--main content wrapper start-->
<div class="main-wrapper">
    <vc:landing-header/>
