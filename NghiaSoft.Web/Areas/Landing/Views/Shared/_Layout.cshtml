    <!--favicon icon-->
    <link rel="icon" href="http://account.thalpha.site/img/favicon.png" type="image/png" sizes="16x16"/>

    <!--title-->
    <title>@ViewBag.Title | NGHIASoft - Powerful Online Solutions</title>

    <!--google fonts-->
    <link
        href="https://fonts.googleapis.com/css2?family=Mulish:wght@400;500;600;700&family=Urbanist:wght@600;700&display=swap"
        rel="stylesheet">

    <!--build:css-->
    <link rel="stylesheet" href="assets/css/main.css"/>
    <!-- endbuild -->

    <!--custom css-->
    <link rel="stylesheet" href="assets/css/custom.css"/>

